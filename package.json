{
  "name": "locatizor",
  "author": "Jason Raimondi <jason@raimondi.us>",
  "description": "Locatizor is a tool to set location data on photos",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf coverage/* dist/* dist-build/*",
    "test": "jest",
    "prestart": "run-s clean webpack:main-dev",
    "start": "run-p webpack:renderer-serve serve",
    "serve": "electron ./dist",
    "electron": "electron-builder --config electron-builder.yml",
    "electron:publish": "electron-builder --config electron-builder.yml -p always",
    "webpack:renderer-serve": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --config build/webpack.renderer.ts",
    "webpack:main-dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config build/webpack.main.ts",
    "webpack:prod": "run-p webpack:main-prod webpack:renderer-prod",
    "webpack:main-prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.main.ts",
    "webpack:renderer-prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.renderer.ts",
    "build:localonly": "run-s clean test webpack:prod electron",
    "build": "run-s clean test webpack:prod",
    "publish": "run-s build electron:publish"
  },
  "devDependencies": {
    "@types/electron-settings": "^3.1.1",
    "@types/jest": "^25.1.3",
    "@types/lodash.findkey": "^4.6.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^13.7.7",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-leaflet": "^2.5.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.0",
    "@types/webpack": "^4.4.22",
    "@types/webpack-merge": "^4.1.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-updater": "^4.0.6",
    "exifreader": "^2.13.1",
    "faker": "^4.1.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "leaflet": "^1.6.0",
    "lodash.findkey": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-leaflet": "^2.6.1",
    "react-router-dom": "^5.1.2",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.4",
    "svg-inline-loader": "^0.8.0",
    "tailwindcss": "^1.2.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.6.0",
    "tslint": "^6.0.0",
    "tslint-loader": "^3.5.0",
    "typescript": "^3.8.3",
    "typescript-plugin-styled-components": "^1.0.0",
    "walkdir": "^0.4.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "electron-settings": "^3.2.0"
  }
}
