{
  "name": "locatizor",
  "title": "Locatizor",
  "version": "0.0.12",
  "description": "Locatizor is a tool to set location data on photos",
  "url": "https://jasonraimondi.github.io/locatizor/",
  "author": {
    "name": "Jason Raimondi",
    "email": "jason@raimondi.us",
    "url": "https://jasonraimondi.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "postversion": "run-s publish",
    "clean": "rm -rf coverage/ dist/ dist-build/",
    "test": "jest",
    "prestart": "run-s clean",
    "start": "run-p serve:renderer webpack:main-watch serve:main",
    "serve:main": "sleep 12 && electron ./dist",
    "serve:renderer": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --config build/webpack.renderer.ts",
    "webpack:main-watch": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --watch --config build/webpack.main.ts",
    "webpack:main-prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.main.ts",
    "webpack:renderer-prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.renderer.ts",
    "webpack:prod": "run-p webpack:main-prod webpack:renderer-prod",
    "electron": "electron-builder --config electron-builder.yml",
    "electron:publish": "electron-builder --config electron-builder.yml -p always",
    "build": "run-s clean test webpack:prod",
    "build:localonly": "run-s clean test webpack:prod electron",
    "publish": "run-s build electron:publish"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/electron-settings": "^3.1.1",
    "@types/jest": "^25.2.1",
    "@types/lodash.findkey": "^4.6.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-leaflet": "^2.5.1",
    "@types/react-router-dom": "^5.1.4",
    "@types/styled-components": "^5.0.0",
    "@types/webpack": "^4.41.10",
    "@types/webpack-merge": "^4.1.3",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.5.2",
    "dayjs": "^1.8.26",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-updater": "^4.2.5",
    "exifreader": "^2.13.1",
    "faker": "^4.1.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "immer": "^6.0.3",
    "inter-ui": "^3.13.1",
    "jest": "^25.3.0",
    "leaflet": "^1.6.0",
    "lodash.findkey": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "probe-image-size": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fetch-autocomplete": "0.0.5",
    "react-image": "^2.4.0",
    "react-leaflet": "^2.6.3",
    "react-router-dom": "^5.1.2",
    "react-visibility-sensor": "^5.1.1",
    "rxjs": "^6.5.5",
    "style-loader": "^1.1.3",
    "styled-components": "^5.1.0",
    "svg-inline-loader": "^0.8.0",
    "tailwindcss": "^1.2.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-loader": "^3.5.0",
    "typescript": "^3.8.3",
    "typescript-plugin-styled-components": "^1.0.0",
    "walkdir": "^0.4.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.5",
    "zondicons": "^1.2.0"
  },
  "dependencies": {
    "electron-settings": "^3.2.0",
    "piexifjs": "^2.0.0-beta.9"
  }
}
